{
  "name": "@agnoc/cli",
  "version": "0.18.0-next.0",
  "description": "Agnoc command line interface",
  "keywords": [
    "agnoc",
    "cli",
    "conga",
    "cecotec",
    "driver",
    "reverse",
    "engineering"
  ],
  "homepage": "https://github.com/adrigzr/agnoc",
  "bugs": {
    "url": "https://github.com/adrigzr/agnoc/issues"
  },
  "license": "MIT",
  "author": "Adrián González Rus <a.gonzalezrus@gmail.com> (https://github.com/adrigzr)",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "agnoc": "./bin/agnoc"
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./test-support": {
      "types": "./lib/test-support.d.ts",
      "default": "./lib/test-support.js"
    }
  },
  "files": [
    "bin",
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adrigzr/agnoc.git",
    "directory": "packages/cli"
  },
  "scripts": {
    "build": "npm-run-all build:lib",
    "build:lib": "tsc -b tsconfig.build.json",
    "clean": "rm -rf .build-cache lib coverage",
    "posttest": "test \"$(cat coverage/coverage-summary.json | json total.lines.total)\" -gt 0",
    "test": "nyc mocha src test"
  },
  "nx": {
    "targets": {
      "build": {
        "inputs": [
          "{workspaceRoot}/package.json",
          "{projectRoot}/src/**/*",
          "{projectRoot}/package.json",
          "{projectRoot}/tsconfig.*"
        ],
        "outputs": [
          "{projectRoot}/lib",
          "{projectRoot}/.build-cache"
        ]
      },
      "test": {
        "dependsOn": [
          "build"
        ],
        "inputs": [
          "{workspaceRoot}/.mocharc.yml",
          "{projectRoot}/src/**/*",
          "{projectRoot}/test/**/*"
        ],
        "outputs": [
          "{projectRoot}/coverage"
        ]
      }
    }
  },
  "dependencies": {
    "@agnoc/toolkit": "^0.18.0-next.0",
    "@agnoc/transport-tcp": "^0.18.0-next.0",
    "chalk": "^4.1.1",
    "cli-ux": "^5.5.1",
    "commander": "^10.0.0",
    "node-wifi": "^2.0.16",
    "tslib": "^2.5.0"
  },
  "optionalDependencies": {
    "pcap": "^3.1.0"
  },
  "devDependencies": {
    "@johanblumenberg/ts-mockito": "^1.0.35",
    "chai": "^4.3.7",
    "execa": "^5.1.1",
    "mocha": "^10.2.0",
    "mock-fs": "^5.2.0",
    "sinon": "^15.0.1",
    "sinon-chai": "^3.7.0"
  },
  "engines": {
    "node": ">=18.12"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "./README.md",
    "displayName": "CLI"
  }
}
