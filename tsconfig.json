{
  "extends": "./tsconfig.base.json",
  "include": ["packages/*/src", "packages/*/test", "packages/**/typings.d.ts"],
  "typedocOptions": {
    "entryPoints": ["packages/cli", "packages/transport-tcp", "packages/domain", "packages/toolkit"],
    "entryPointStrategy": "packages",
    "out": "public/typedoc",
    "excludePrivate": false,
    "excludeProtected": false,
    "excludeInternal": false,
    "includeVersion": true,
    "sort": ["kind", "instance-first", "static-first"],
    "hideGenerator": true,
    "noMissingExports": true
  }
}
